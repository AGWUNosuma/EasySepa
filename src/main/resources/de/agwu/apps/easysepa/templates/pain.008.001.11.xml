<?xml version="1.0" encoding="UTF-8"?>
<Document xmlns="urn:iso:std:iso:20022:tech:xsd:pain.008.001.11" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <CstmrDrctDbtInitn>
    <GrpHdr>
      <MsgId>{{msgId}}</MsgId>
      <CreDtTm>{{creationDateTime}}</CreDtTm>
      <NbOfTxs>{{numberOfTransactions}}</NbOfTxs>
      <CtrlSum>{{controlSum}}</CtrlSum>
      <InitgPty>
        <Nm>{{initiatorName}}</Nm>
      </InitgPty>
    </GrpHdr>
    <PmtInf>
      <PmtInfId>{{pmtInfId}}</PmtInfId>
      <PmtMtd>DD</PmtMtd>
      <BtchBookg>{{batchBooking}}</BtchBookg>
      <NbOfTxs>{{numberOfTransactions}}</NbOfTxs>
      <CtrlSum>{{controlSum}}</CtrlSum>
      <PmtTpInf>
        <SvcLvl>
          <Cd>SEPA</Cd>
        </SvcLvl>
        <LclInstrm>
          <Cd>{{localInstrumentCode}}</Cd>
        </LclInstrm>
        <SeqTp>{{seqTp}}</SeqTp>
      </PmtTpInf>
      <ReqdColltnDt>{{reqdColltnDt}}</ReqdColltnDt>
      <Cdtr>
        <Nm>{{creditorName}}</Nm>
      </Cdtr>
      <CdtrAcct>
        <Id>
          <IBAN>{{creditorIBAN}}</IBAN>
        </Id>
      </CdtrAcct>
      <CdtrAgt>
        <FinInstnId>
          {{#creditorBIC}}
          <BICFI>{{creditorBIC}}</BICFI>
          {{/creditorBIC}}
          {{^creditorBIC}}
          <Othr>
            <Id>NOTPROVIDED</Id>
          </Othr>
          {{/creditorBIC}}
        </FinInstnId>
      </CdtrAgt>
      <ChrgBr>SLEV</ChrgBr>
      <CdtrSchmeId>
        <Id>
          <PrvtId>
            <Othr>
              <Id>{{creditorId}}</Id>
              <SchmeNm>
                <Prtry>SEPA</Prtry>
              </SchmeNm>
            </Othr>
          </PrvtId>
        </Id>
      </CdtrSchmeId>
      {{#transactions}}
      <DrctDbtTxInf>
        <PmtId>
          <EndToEndId>{{endToEndId}}</EndToEndId>
        </PmtId>
        <InstdAmt Ccy="EUR">{{amount}}</InstdAmt>
        <DrctDbtTx>
          <MndtRltdInf>
            <MndtId>{{mandateId}}</MndtId>
            <DtOfSgntr>{{mandateSignatureDate}}</DtOfSgntr>
          </MndtRltdInf>
        </DrctDbtTx>
        <DbtrAgt>
          <FinInstnId>
            {{#debtorBIC}}
            <BICFI>{{debtorBIC}}</BICFI>
            {{/debtorBIC}}
            {{^debtorBIC}}
            <Othr>
              <Id>NOTPROVIDED</Id>
            </Othr>
            {{/debtorBIC}}
          </FinInstnId>
        </DbtrAgt>
        <Dbtr>
          <Nm>{{debtorName}}</Nm>
        </Dbtr>
        <DbtrAcct>
          <Id>
            <IBAN>{{debtorIBAN}}</IBAN>
          </Id>
        </DbtrAcct>
        {{#remittanceInfo}}
        <RmtInf>
          <Ustrd>{{remittanceInfo}}</Ustrd>
        </RmtInf>
        {{/remittanceInfo}}
      </DrctDbtTxInf>
      {{/transactions}}
    </PmtInf>
  </CstmrDrctDbtInitn>
</Document>
